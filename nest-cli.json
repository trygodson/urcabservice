{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/user_api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/user_api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/user_api",
  "projects": {
    "user_api": {
      "type": "application",
      "root": "apps/user_api",
      "entryFile": "main",
      "sourceRoot": "apps/user_api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user_api/tsconfig.app.json"
      }
    },
    "driver_api": {
      "type": "application",
      "root": "apps/driver_api",
      "entryFile": "main",
      "sourceRoot": "apps/driver_api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/driver_api/tsconfig.app.json"
      }
    },
    "admin_api": {
      "type": "application",
      "root": "apps/admin_api",
      "entryFile": "main",
      "sourceRoot": "apps/admin_api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin_api/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
   
   
  }
}